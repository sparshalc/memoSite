{
    "manifest_version": 2,
    "name": "MemoSite",
    "version": "1.0",
    "description": "Save and export important site URLs",
    "permissions": ["storage", "activeTab"],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  